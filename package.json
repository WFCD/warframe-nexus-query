{
  "name": "warframe-nexus-query",
  "version": "1.6.5",
  "description": "Query prices form nexus-stats.com and warframe.market.",
  "main": "index.js",
  "scripts": {
    "test": "npx nyc --reporter=text mocha --exit",
    "coverage": "npm test && npx nyc report --reporter=text-lcov | npx coveralls",
    "build-docs": "npx jsdoc -t ./node_modules/ink-docstrap/template -c jsdoc.conf -R README.md -r index.js lib/. -d docs",
    "lint": "npx eslint index.js lib/ test/",
    "lint:fix": "npx eslint index.js lib/ test/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WFCD/warframe-nexus-query.git"
  },
  "keywords": [
    "warframe",
    "trading",
    "warframe-nexus",
    "warframe-trading"
  ],
  "author": "tobitenno",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/WFCD/warframe-nexus-query/issues"
  },
  "homepage": "https://github.com/WFCD/warframe-nexus-query#README",
  "dependencies": {
    "get-image-colors": "^1.8.1",
    "image-downloader": "^3.4.2",
    "json-fetch-cache": "^1.2.3",
    "json-query": "^2.2.2",
    "nexushub-client": "^1.1.1",
    "node-fetch": "^2.6.0",
    "node-md-config": "^2.0.1"
  },
  "devDependencies": {
    "@types/json-query": "^2.2.0",
    "@types/node-fetch": "^2.5.4",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.3",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.1.0"
  },
  "release": {
    "branch": "master"
  }
